{
  "name": "ytube_clone_api",
  "version": "1.0.0",
  "description": "ytube_clone API server",
  "keywords": [
    "api",
    "nestjs"
  ],
  "license": "MIT",
  "author": "shyunjan",
  "module": "esnext",
  "type": "commonjs",
  "main": "main.js",
  "scripts": {
    "build": "rimraf dist && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start.watch": "nest start --watch",
    "start:local": "rimraf dist && nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "rimraf dist && nest build --webpack --webpackPath webpack-hmr.config.js --watch --debug",
    "start:dev": "rimraf dist && nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:prod": "rimraf dist && node dist/main",
    "local": "rimraf dist && webpack --config webpack.config.js --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@swc/cli": "^0.7.10",
    "@swc/core": "^1.15.8",
    "@types/node": "^25.0.8",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^17.0.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "prettier-eslint": "^16.4.2",
    "rimraf": "^6.1.2",
    "run-script-webpack-plugin": "^0.2.3",
    "swc-loader": "^0.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/request-context": "^6.2.1",
    "@fastify/static": "^9.0.0",
    "@nestjs/common": "^11.1.11",
    "@nestjs/core": "^11.1.11",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-fastify": "^11.1.11",
    "fastify": "^5.6.2",
    "nest-winston": "^1.10.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
